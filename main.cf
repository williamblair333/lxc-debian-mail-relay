
inet_interfaces = all
inet_protocols = ipv4

relayhost = my.maildomain.com
relay_domains=maildomain.com

smtp_use_tls=yes
smtp_tls_security_level=encrypt

myorigin = /etc/mailname
myhostname=relay1.maildomain.com
mydestination =

mailbox_size_limit = 0
recipient_delimiter = +
compatibility_level=2
